    "moment": "^2.18.1",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-flexbox-grid": "^1.1.2",
    "react-redux": "^5.0.4",
